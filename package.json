{
  "name": "gsabot",
  "version": "0.1.4",
  "description": "Discord bot for the Gender and Sexuality Alliance.",
  "main": "",
  "dependencies": {
    "better-sqlite3": "^8.5.1",
    "canvas": "^2.11.2",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "scripts": {
    "server": "node server/index.js",
    "server-dev": "nodemon --quiet server",
    "client": "npm run start --prefix client",
    "client-dev": "npm run dev --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "initialize": "concurrently \"node utils/db.utils/createTables.js\" \"npm run initialize --prefix client\"",
    "reset": "npm reset --prefix client",
    "lint": "eslint ."
  },
